
@{
    ViewBag.Title = "Ana Sayfa";
    Layout = "~/Views/Shared/_SiteLayout.cshtml";
}

<section id="featured">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">

                @*Main Slider*@
                @{
                    Html.RenderAction("PartialSlider", "Home");
                 }

            </div>
        </div>
    </div>
</section>

<section class="callaction">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="big-cta">
                    <div class="cta-text">
                        <h2>  <span><a target="_blank" href="https://ieeeytu.com">@ViewBag.ID.Title</a></span>  @ViewBag.ID.Keywords</h2>
                        <h3>@ViewBag.ID.Slogan</h3>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section id="content">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="solidline">
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <h4 class="heading">Neler Yapıyoruz?</h4> <br /><br />
                <div class="row">
                    @*Services*@
                    @{
                        Html.RenderAction("PartialWork", "Home");
                    }
                </div>
            </div>
        </div>
    </div>
</section>
